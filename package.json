{
  "name": "redux-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint js",
    "lint:autocorrect": "eslint js --fix",
    "build:clean": "rm build/*.js build/*.map build/*.css || :",
    "build:js": "NODE_ENV=production NODE_PATH=./js browserify js/index.js -t babelify -t aliasify -g envify -g uglifyify --debug | exorcist build/bundle.js.map -b . > build/bundle.js",
    "build:styles": "node-sass styles/main.sass | postcss --use autoprefixer -o build/main.css",
    "build": "npm run build:clean && npm run lint && npm run build:js && npm run build:styles",
    "watch:js": "NODE_ENV=development NODE_PATH=./js watchify js/index.js -o build/bundle.js -t babelify -t aliasify -v --debug",
    "watch:styles": "touch .temp/main.css & node-sass --watch styles/ -o .temp & postcss --watch --use autoprefixer -o build/main.css .temp/main.css",
    "watch:lint": "esw -w js",
    "serve": "live-server --entry-file=index.html --ignore='js,styles,.temp' .",
    "dev": "npm run watch:lint & npm run watch:js & npm run watch:styles & npm run serve",
    "deploy": "npm run build && gulp deploy",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "watch:test": "npm test -- --watch"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "aliasify": "1.9.0",
    "autoprefixer": "6.3.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "redbox-react": "1.2.2",
    "react-transform-catch-errors": "1.0.2",
    "browserify": "13.0.0",
    "envify": "3.4.0",
    "enzyme": "2.2.0",
    "eslint": "2.5.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.2.3",
    "eslint-watch": "2.1.10",
    "event-stream": "3.3.2",
    "exorcist": "0.4.0",
    "expect": "1.16.0",
    "gulp": "3.9.1",
    "gulp-awspublish": "3.0.1",
    "gulp-rev-all": "0.8.24",
    "live-server": "0.9.2",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "postcss-cli": "2.3.3",
    "postcss-cli": "2.5.1",
    "react-addons-test-utils": "0.14.7",
    "uglify-js": "2.6.2",
    "uglifyify": "3.0.1",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-polyfill": "6.7.4",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "redux": "3.3.1",
    "redux-act": "0.4.0",
    "redux-thunk": "2.0.1",
    "updeep": "0.14.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": [
      "./bower_components/**/*",
      "./js/vendor/**/*"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-catch-errors",
                  "imports": [
                    "react",
                    "redbox-react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "arrow-body-style": 0,
      "react/jsx-no-bind": 0,
      "react/prefer-stateless-function": 0,
      "space-before-function-paren": [
        2,
        "never"
      ]
    }
  },
  "aliasify": {
    "aliases": {}
  }
}
